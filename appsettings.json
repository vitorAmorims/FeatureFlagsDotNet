{
  "FeatureManagement": {
    "BooleanFilter": false,
    "PercentageFilter": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "CustomFilter": {
      "EnabledFor": [
        {
          "Name": "LanguageFilter",
          "Parameters": {
            "AllowedLanguages": [
              "en-GB",
              "en-US"
            ]
          }
        }
      ]
    },
    "TimeWindowFilter": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "Start": "2022-09-12T18:56:00+00:00",
            "End": "2022-09-13T18:59:00+00:00"
          }
        }
      ]
    },
    "TargetingFilter": {
      "EnabledFor": [
        {
          "Name": "Targeting",
          "Parameters": {
            "Audience": {
              "Users": [
                "user@code-maze.com"
              ]
            }
          }
        }
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
